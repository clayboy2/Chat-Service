      <TextField fx:id="portBox" layoutX="236.0" layoutY="28.0" prefHeight="25.0" prefWidth="48.0" promptText="1120" />
      <Label layoutX="211.0" layoutY="32.0" text="Port:" />
      <Button layoutX="130.0" layoutY="88.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Connect" />
      <Label fx:id="responder" layoutX="146.0" layoutY="120.0" />
